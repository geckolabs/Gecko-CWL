{
    "name": "geckolabs/cwh",
    "homepage": "https://github.com/geckolabs/Gecko-CWL",
    "type": "library",
    "description": "AWS CloudWatch Handler for Monolog library",
    "keywords": [
        "aws",
        "cloudwatch",
        "monolog"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Gecko Labs",
            "email": "devs@geckolabs.co.uk",
            "homepage": "https://geckoengage.com"
        }
    ],
    "support": {
        "issues": "https://github.com/geckolabs/Gecko-CWL/issues",
        "source": "https://github.com/geckolabs/Gecko-CWL"
    },
    "require": {
        "php": "^7.2",
        "monolog/monolog": "^2.0",
        "aws/aws-sdk-php": "^3.18"
    },
    "require-dev": {
        "maxbanton/dd": "^2.0",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "suggest": {},
    "config": {
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Maxbanton\\Cwh\\": "src"
        }
    },
    "minimum-stability": "stable"
}
